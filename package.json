{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript node js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devinkel/typescript-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Kelvin Rosa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devinkel/typescript-api/issues"
  },
  "homepage": "https://github.com/devinkel/typescript-api#readme",
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
